{
  "name": "@dawaltconley/header-basic",
  "version": "0.2.4",
  "description": "A basic header for websites.",
  "main": "index.mjs",
  "exports": {
    "import": "./index.mjs",
    "require": "./index.cjs"
  },
  "files": [
    "_header.scss",
    "dist/**/*"
  ],
  "scripts": {
    "build": "npx gulp build",
    "test": "npx gulp eleventy; npx @11ty/eleventy",
    "serve": "npx gulp eleventyWatch & npx @11ty/eleventy --serve",
    "document": "npx documentation readme index.mjs --pe cjs --section Components --markdown-toc-max-depth 2",
    "prepublishOnly": "rm -r dist; npm run build; npm run test"
  },
  "author": "Dylan Awalt-Conley",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@babel/core": "^7.16.12",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/preset-env": "^7.16.11",
    "@fortawesome/fontawesome-pro": "^5.15.3",
    "documentation": "^13.2.5",
    "dot": "^1.1.3",
    "eslint": "^7.31.0",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.0.0",
    "js-yaml": "^3.14.1",
    "sass": "^1.36.0",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-scss": "^3.20.1"
  },
  "dependencies": {
    "liquid-args": "^1.0.0"
  }
}
